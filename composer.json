{
    "name": "rowbot/dom",
    "description": "A modern alternative to PHP's built in DOM classes.",
    "authors": [
        {
            "name": "Trevor Rowbotham",
            "homepage": "http://trowbotham.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "rowbot/url": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "symfony/var-dumper": "^3.2",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Rowbot\\DOM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rowbot\\DOM\\Tests\\": "tests/"
        }
    }
}
